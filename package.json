{
  "name": "@benliam12/ngx-electron",
  "description": "A wrapper for Electron Renderer APIs. Finally easy to use in Angular forked from ThorstenHans/ngx-electron",
  "version": "1.0.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Benliam12/ngx-electron.git"
  },
  "files": [
    "dist/@benliam12/ngx-electron",
    "README.md"
  ],
  "author": "Thorsten Hans <thorsten.hans@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-win": "ng build && copy README.md dist\\@benliam12\\ngx-electron\\README.md",
    "build": "ng build && cp README.md dist\\@benliam12\\ngx-electron\\README.md",
    "commit": "git-cz",
    "lint": "ng lint",
    "start": "ng serve",
    "test": "ng test",
    "update": "ng update @angular/cli@11"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "angular",
    "ng",
    "ngx",
    "electron",
    "atom-shell",
    "github-electron"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "bugs": {
    "url": "https://github.com/Benliam12/ngx-electron/issues"
  },
  "dependencies": {
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "core-js": "^3.2.1",
    "rxjs": "^6.5.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.8",
    "electron": "^6.0.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular-devkit/build-ng-packagr": "^0.1002.4",
    "@angular/cli": "^15.2.10",
    "@angular/compiler-cli": "^15.2.10",
    "@angular/language-service": "^15.2.10",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.5",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^15.2.2",
    "ts-node": "^8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.9.5"
  }
}